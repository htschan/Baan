{
  "name": "baan",
  "version": "0.0.1",
  "author": "Hans Tschan",
  "homepage": "https://baan.angusido.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "prebuild": "node version.js",
    "build": "ionic-app-scripts build",
    "postbuild": "workbox injectManifest",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "open": "http-server www -o -a localhost -p 1234",
    "hashcb": "hashmark -l 8 -r --cwd www/build \"*.{js,css}\" {name}.{hash}{ext} | map-replace -m \"<[^>]+>\" www/index.html",
    "dist": "npm run clean && shx rm -rf www/* && npm run build && npm run hashcb && workbox injectManifest"
  },
  "config": {
    "ionic_copy": "./copy.config.js"
  },
  "dependencies": {
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@ionic/storage": "2.1.3",
    "@types/googlemaps": "^3.30.8",
    "angularfire2": "^5.0.0-rc.5-next",
    "firebase": "^4.13.1",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "promise-polyfill": "^7.1.2",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "workbox-sw": "^3.2.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^3.1.9",
    "fs-extra": "^5.0.0",
    "git-describe": "^4.0.2",
    "hashmark": "^5.0.0",
    "http-server": "^0.11.1",
    "ionic": "3.20.0",
    "map-replace": "^1.0.2",
    "path": "^0.12.7",
    "shx": "0.2.2",
    "typescript": "2.4.2",
    "workbox-cli": "^3.2.0"
  },
  "description": "A Baan Ionic project"
}
